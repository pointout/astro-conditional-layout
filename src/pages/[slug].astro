---
import BaseLayout from '../layouts/BaseLayout.astro'

export async function getStaticPaths() {
    return [
        { params: { slug:'page1' }, props: { Prop1:'Page1' } },
        { params: { slug:'page2' }, props: { Prop2:'Page2' } }
    ]
}
const {Prop1, Prop2} = Astro.props
---
{ Prop1 &&
<BaseLayout title={Prop1}>
    <h1>{Prop1}</h1>
</BaseLayout>
}
{ Prop2 &&
<BaseLayout title={Prop2}>
    <h1>{Prop2}</h1>
</BaseLayout>
}
