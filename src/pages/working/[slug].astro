---
import BaseLayout from '../../layouts/BaseLayout.astro'

export async function getStaticPaths() {
    return [
        { params: { slug:'page1' }, props: { Prop1:'Page1 working' } },
        { params: { slug:'page2' }, props: { Prop2:'Page2 working' } }
    ]
}
const {Prop1 = "", Prop2 = ""} = Astro.props
---
<BaseLayout title={`${Prop1}${Prop2}`}>
    <h1>{`${Prop1}${Prop2}`}</h1>
</BaseLayout>
